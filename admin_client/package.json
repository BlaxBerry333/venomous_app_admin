  "description": "blaxberry333's venomous apps admin client",
  "type": "module",
  "scripts": {
    "start:dev": "vite --host --mode development",
    "start:prod": "vite --host --mode production",
    "build:dev": "tsc && vite build --mode development",
    "build:prod": "tsc && vite build --mode production",
    "build:storybook": "storybook build",
    "dev:storybook": "storybook dev -p 3100",
    "preview": "vite preview",
    "commit": "cd .. && npm run commit",
    "check-packages": "npx depcheck",
    "check-type": "tsc --noEmit",
    "check-eslint": "eslint . --cache --cache-location=./.cache/.eslintcache",
    "check-stylelint": "stylelint \"**/*.{css,scss}\" --allow-empty-input --cache --cache-location=./.cache/.stylelintcache",
    "check-prettier": "prettier . --check",
    "check-all": "concurrently --names \"check-type,check-eslint,check-stylelint,check-prettier\" \"yarn check-type\" \"yarn check-eslint\" \"yarn check-stylelint\" \"yarn check-prettier\"",
    "eslint": "prettier --write .",
    "stylelint": "stylelint \"**/*.{css,scss}\" --fix --allow-empty-input --cache --cache-location=./.cache/.stylelintcache",
    "prettier": "prettier . --write --cache --cache-location=./.cache/.prettiercache",
    "format-all": "concurrently  --names \"eslint,stylelint,prettier\" \"yarn eslint\" \"yarn stylelint\" \"yarn prettier\"",
    "clean-cache": "rm -rf .cache"
  },
